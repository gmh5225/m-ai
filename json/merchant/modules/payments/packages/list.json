{
    "name": "Gói Thue bao",
    "package": "payments.packages",
    "use_trash": false,
    "titles": {
        "default": "Danh sách Gói thuê bao",
        "trash": "Danh sách Gói thue bao đã xóa"
    },
    "data": [],
    "filter": {
        "search_columns": [],
        "sort_columns": []
    },
    "table": {
        "class": "header-center",
        "columns": [
            {
                "title": "Tên gói",
                "class": "text-center",
                "text": ":name"
            },
            {
                "title": "Mô tả",
                "class": "text-center",
                "text": ":description"
            },
            {
                "title": "Số tháng",
                "class": "text-center",
                "text": ":quantity"
            },
            {
                "title": "Giá bán buôn",
                "class": "text-center",
                "template": "{@get_price_format}",
                "data": {
                    "@get_price_format": [
                        ":wholesale_price",
                        ":currency"
                    ]
                }
            },
            {
                "title": "Giá bán lẻ",
                "class": "text-center",
                "template": "{@get_price_format}",
                "data": {
                    "@get_price_format": [
                        ":retail_price",
                        ":currency"
                    ]
                }
            }
        ]
    },
    "resources": {
        "js_data": [],
        "js": [],
        "css": []
